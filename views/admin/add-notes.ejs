                        <div class="inputBox">
                            <input  name="course" type="text" required="required">
                                <!-- <option value="btech-CSE">BTECH computer science and Engineering</option> -->
                            </select>
                            <span>Course</span>
                            <i></i>
                        </div>
                    </div>

                    <div class="field-col">
                        <div class="inputBox">
                            <input name="semester" type="text" required="required">
